<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomsRaidMode">


    <ImageView
        android:id="@+id/LighthouseMap"
        android:layout_width="732dp"
        android:layout_height="382dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lighthousemap"
        android:contentDescription="@string/Lighthouse" />

    <RelativeLayout
        android:id="@+id/Lighthouse_PaintViewer"
        android:layout_width="732dp"
        android:layout_height="382dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <include layout="@layout/paintviewer"/>

    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout >